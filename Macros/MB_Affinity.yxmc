    <Node ToolID="10">
      <GuiSettings Plugin="AlteryxRPluginGui.R">
        <Position x="906" y="630" />
      </GuiSettings>
      <Properties>
        <Configuration>
          <RScript><![CDATA[# Processing the first chunk of data
X <- as.matrix(read.Alteryx.First("#1", %Question.chunk.size%))
X[is.na(X)] <- 0
n_rows <- nrow(X)
iota <- matrix(1, nrow = 1, ncol = n_rows)
XtX <- t(X) %*% X
if ('%Question.co-occurance%' != "True") {
	Y <- iota %*% X
}
# Process the remaining chunks of the data
while (!is.null(the_data <- read.Alteryx.Next("#1"))) {
	X <- as.matrix(the_data)
	X[is.na(X)] <- 0
  n_rows <- nrow(X)
  iota <- matrix(1, nrow = 1, ncol = n_rows)
  XtX <- XtX + t(X) %*% X
	if ('%Question.co-occurance%' != "True") {
  	Y <- Y + (iota %*% X)
	}
}
# Calculate the similarity or distance matrix
if ('%Question.co-occurance%' == "True") {
	the_matrix <- XtX
} else {
	the_matrix <- XtX/(sqrt(t(Y) %*% Y))
	if ('%Question.distance%' == "True") {
  	the_matrix <- 1 - the_matrix
	}
}
write.Alteryx(as.data.frame(the_matrix), 1, TRUE)
]]></RScript>
